{
  "name": "nuxtify",
  "version": "1.0.0",
  "description": "Nuxtify",
  "author": "Francois Levesque <francois@witify.io>",
  "engines": {
    "node": ">=11.0.0"
  },
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt",
    "build": "cross-env NODE_ENV=production nuxt build",
    "start": "cross-env NODE_ENV=production nuxt start --port 3000",
    "generate": "cross-env NODE_ENV=production nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^13.4.0",
    "@nuxtjs/axios": "^5.6.0",
    "@nuxtjs/component-cache": "^1.1.5",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/google-gtag": "^1.0.4",
    "@nuxtjs/sentry": "^3.0.0",
    "bulma": "^0.7.5",
    "contentful": "^7.10.0",
    "nuxt": "^2.9.2",
    "nuxt-env": "^0.1.0",
    "nuxt-facebook-pixel-module": "^1.3.0",
    "nuxt-i18n": "^6.3.0",
    "vue-scrollto": "^2.17.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.4.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "glob-all": "^3.1.0",
    "node-sass": "^4.12.0",
    "purgecss-webpack-plugin": "^1.6.0",
    "sass-loader": "^7.3.1"
  }
