    "vue-scrollto": "^2.11.0",
    "vue-typer": "^1.2.0"
  },
  "devDependencies": {
    "@nuxtjs/component-cache": "^1.1.2",
    "@nuxtjs/sentry": "^1.1.2",
    "babel-eslint": "^8.2.1",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.0",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "glob-all": "^3.1.0",
